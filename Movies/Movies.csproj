<Project Sdk="Microsoft.NET.Sdk">

    <ItemGroup>
        <None Remove="movies.csv" />
    </ItemGroup>

    <ItemGroup>
        <EmbeddedResource Include="movies.csv">
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        </EmbeddedResource>
    </ItemGroup>

    <ItemGroup>
        <None Remove="movies2.csv" />
    </ItemGroup>

    <ItemGroup>
        <EmbeddedResource Include="movies2.csv">
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        </EmbeddedResource>
    </ItemGroup>

    <ItemGroup>
        <None Remove="shows.csv" />
    </ItemGroup>

    <ItemGroup>
        <EmbeddedResource Include="shows.csv">
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        </EmbeddedResource>
    </ItemGroup>

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="6.0.2-mauipre.1.22054.8" />
    </ItemGroup>

</Project>
